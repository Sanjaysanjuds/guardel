document.getElementById('para2').innerHTML = days[d.getDay()]+', '+d.getDate()+' '+months[d.getMonth()]+' '+' '+d.getFullYear()+' ';
}
function seconds_with_leading_zeros(dt) 
{ 
return /\((.*)\)/.exec(new Date().toString())[1];
}
